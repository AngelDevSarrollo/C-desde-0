#include <stdio.h>
using namespace std;

int main() {
	//1.Declaramos la variable
	int miNumero;// tiene un valor basura
	printf("%d", miNumero);// 4194432 valor aleatorio que esta almacenado en la memoria(basura)
	
	//2. La inicializamos
	miNumero = 10;
	//3. Imprimimos la variable
	// farmato %d -> decimal o int
	//Caracter especial \n salto de linea
	printf("\n%d", miNumero);
	// 4. Modificamos el valor
	miNumero = 20;//Se sobreescribe el valor de 10
	printf("\n%d", miNumero);
	
	
	return 0;
}

